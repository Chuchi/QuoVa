        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="left"
            android:weightSum="6"
            android:transitionGroup="false"
            android:padding="5dp"
            android:layout_marginTop="5dp"
