                android:layout_height="match_parent"
                android:weightSum="2"
                android:layout_weight="4.5"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_gravity="left"
                android:paddingTop="2dp">

                <LinearLayout
                    android:orientation="horizontal"
